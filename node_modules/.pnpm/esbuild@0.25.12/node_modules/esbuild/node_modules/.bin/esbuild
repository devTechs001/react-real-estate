#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/darkhat/projects/react-projects/real-estate/react-real-estate/node_modules/.pnpm/esbuild@0.25.12/node_modules/esbuild/bin/node_modules:/home/darkhat/projects/react-projects/real-estate/react-real-estate/node_modules/.pnpm/esbuild@0.25.12/node_modules/esbuild/node_modules:/home/darkhat/projects/react-projects/real-estate/react-real-estate/node_modules/.pnpm/esbuild@0.25.12/node_modules:/home/darkhat/projects/react-projects/real-estate/react-real-estate/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/darkhat/projects/react-projects/real-estate/react-real-estate/node_modules/.pnpm/esbuild@0.25.12/node_modules/esbuild/bin/node_modules:/home/darkhat/projects/react-projects/real-estate/react-real-estate/node_modules/.pnpm/esbuild@0.25.12/node_modules/esbuild/node_modules:/home/darkhat/projects/react-projects/real-estate/react-real-estate/node_modules/.pnpm/esbuild@0.25.12/node_modules:/home/darkhat/projects/react-projects/real-estate/react-real-estate/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
"$basedir/../../bin/esbuild"   "$@"
exit $?
