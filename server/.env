NODE_ENV=development
PORT=5000
MONGODB_URI=mongodb://localhost:27017/realestate
MONGO_URI=mongodb://localhost:27017/realestate
JWT_SECRET=your_super_secret_jwt_key_change_in_production
JWT_EXPIRE=30d

# Cloudinary
CLOUDINARY_CLOUD_NAME=demo_cloud_name
CLOUDINARY_API_KEY=123456789012345
CLOUDINARY_API_SECRET=abcdeghijklmnopqrstuvwxyz

# Email
EMAIL_HOST=smtp.gmail.com
EMAIL_PORT=587
EMAIL_USER=your_email@gmail.com
EMAIL_PASSWORD=your_app_password
EMAIL_FROM=noreply@realestateapp.com

# Client URL
CLIENT_URL=http://localhost:5174

# Stripe Configuration
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key_here
STRIPE_BASIC_PRICE_ID=price_basic_demo
STRIPE_PRO_PRICE_ID=price_pro_demo
STRIPE_PREMIUM_PRICE_ID=price_premium_demo

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key
OPENAI_MODEL=gpt-4-turbo-preview
OPENAI_VISION_MODEL=gpt-4-vision-preview

# Google Cloud Vision
GOOGLE_CLOUD_PROJECT_ID=your_project_id
GOOGLE_APPLICATION_CREDENTIALS=path/to/credentials.json

# Redis for AI Caching
REDIS_URL=redis://localhost:6379
REDIS_PASSWORD=your_redis_password

# AI Features Toggle
AI_CHATBOT_ENABLED=true
AI_PRICE_PREDICTION_ENABLED=true
AI_FRAUD_DETECTION_ENABLED=true
AI_IMAGE_ANALYSIS_ENABLED=true
AI_RECOMMENDATIONS_ENABLED=true

# AI Rate Limiting
AI_REQUESTS_PER_HOUR=100
AI_REQUESTS_PER_DAY=1000

# TensorFlow Settings
TF_ENABLE_ONEDNN_OPTS=0
